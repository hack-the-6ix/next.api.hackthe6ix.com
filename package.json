    "check-password-strength": "^2.0.7",
    "nestjs-zod": "^3.0.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
